-	set_baseband_phy_config(adapt);
-
-	/* If EEPROM or EFUSE autoload OK, We must config by PHY_REG_PG.txt */
-	if (!eeprom->bautoload_fail_flag) {
-		hal_data->pwrGroupCnt = 0;
-		config_bb_with_pgheader(adapt);
